    <buffers>
		<buffer name="quadBuf" >
            <file name="buffers/square.buf"/>
            <DIM x=13 y=1 z=1 />
			<structure>
				<field value="FLOAT" />
			</structure>
        </buffer>	
        <!--Debug Buffer-->
		<!--<buffer name="debugBufferMesh">-->
			<!--<DIM x="262144" y="1" z=1 />-->
			<!--<structure>-->
				<!--<field value="VEC4" />-->
			<!--</structure>-->
		<!--</buffer>-->
    </buffers>
	
    <materials>

		<material name="tradTessQuad">
			<shader name="tradTessQuad">
				<values>
                    <valueof uniform="projViewModelMatrix"  
                             type="RENDERER" context="CURRENT" 
							 component="PROJECTION_VIEW_MODEL" />

                    <valueof uniform="icols" 
                             type="RENDERER" context="CURRENT" 
                             component="icols"  />

                    <valueof uniform="irows" 
                             type="RENDERER" context="CURRENT" 
                             component="irows"  />

                     <valueof uniform="olevel0" 
                             type="RENDERER" context="CURRENT" 
                             component="olevel0"  />

		            <valueof uniform="olevel1" 
                             type="RENDERER" context="CURRENT" 
                             component="olevel1"  />    

                    <valueof uniform="olevel2" 
                             type="RENDERER" context="CURRENT" 
                             component="olevel2"  />

                     <valueof uniform="olevel3" 
                             type="RENDERER" context="CURRENT" 
							 component="olevel3"  />

                    <valueof uniform="texUnit" 		
                             type="TEXTURE_BINDING" context="CURRENT" 
							 component="UNIT" id="0" />
				</values>
			</shader>
			<textures>
				<texture UNIT=0 name="tex" />
			</textures>
        </material>

        <!--32 -->
        <material name="meshOuter32">

            <buffers>
                <buffer name="quadBuf">
					<TYPE value="SHADER_STORAGE" />
    				<BINDING_POINT value="1" />
