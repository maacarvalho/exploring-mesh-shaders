<?xml version="1.0" ?>
<materialLib name="tessellation">

	<textures>
		<texture name="tex" filename="textures/calcada.jpg" mipmap=true/>		
	</textures>

	<shaders>
        <shader name="tradTessQuad" 	ps="shaders/lightTexture.frag" 
									    tc="shaders/simpleQuad.tesc"
									    te="shaders/simpleQuad.tese"
									    vs="shaders/tessDemo.vert" />

        <shader name="meshInner32" 	        ts = "shaders/innerTessQuad.32.task" 
                                            ms = "shaders/innerTessQuad.32.mesh" 
                                            ps = "shaders/lightTexture.frag" />
        
        <shader name="meshOuter32" 	        ts = "shaders/outerTessQuad.32.task" 
                                            ms = "shaders/outerTessQuad.32.mesh" 
                                            ps = "shaders/lightTexture.frag" />

        <shader name="meshInner64" 	        ts = "shaders/innerTessQuad.64.task" 
                                            ms = "shaders/innerTessQuad.64.mesh" 
                                            ps = "shaders/lightTexture.frag" />
        
        <shader name="meshOuter64" 	        ts = "shaders/outerTessQuad.64.task" 
                                            ms = "shaders/outerTessQuad.64.mesh" 
                                            ps = "shaders/lightTexture.frag" />

